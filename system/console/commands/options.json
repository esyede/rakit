{
    "--env=": {
        "description": "Switch environment.",
        "command": "php rakit serve --env=local"
    },
    "--database=": {
        "description": "Change default database connection.",
        "command": "php rakit command --database=mysql"
    },
    "--verbose=true": {
        "description": "Verbose output for package:install.",
        "command": "php rakit package:install foobar --verbose=true"
    },
    "--host=": {
        "description": "Change default host.",
        "command": "php rakit serve --host=0.0.0.0"
    },
    "--port=": {
        "description": "Change default port.",
        "command": "php rakit serve --port=9000"
    }
}
